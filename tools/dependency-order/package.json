{
  "name": "dependency-order",
  "version": "1.1.2",
  "description": "List monorepo packages in dependency order.",
  "engines": {
    "node": ">=14"
  },
  "type": "module",
  "author": "jacobley",
  "license": "MIT",
  "repository": "jacobley/jacobley",
  "homepage": "https://github.com/JacobLey/jacobley/tree/main/tools/dependency-order#readme",
  "bugs": {
    "url": "https://github.com/jacobley/jacobley/issues"
  },
  "keywords": [
    "monorepo",
    "packages",
    "dependency",
    "graph",
    "order"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "coverage": "rushx instrument && nyc mocha ./nyc/tests/unit",
    "instrument": "nyc instrument --nycrc-path=nyc.instrument.cjs ./dist ./nyc",
    "lint": "rush lint-package",
    "test": "rushx coverage",
    "unit-test": "mocha ./dist/tests/unit"
  },
  "main": "./dist/dependency-order.js",
  "exports": {
    ".": {
      "types": [
        "./src/dependency-order.ts",
        "./dist/dependency-order.d.ts"
      ],
      "default": "./dist/dependency-order.js"
    }
  },
  "dependencies": {
    "named-patch": "workspace:^",
    "packages-list": "workspace:^"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "@types/sinon": "^10.0.11",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "typescript": "^4.7.2"
  }
}
