{
  "name": "entry-script",
  "version": "2.1.1",
  "description": "Modular control for entry script execution.",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "author": "jacobley",
  "license": "MIT",
  "repository": "jacobley/jacobley",
  "homepage": "https://github.com/JacobLey/jacobley/tree/main/tools/entry-script#readme",
  "bugs": {
    "url": "https://github.com/jacobley/jacobley/issues"
  },
  "keywords": [
    "executable",
    "entry point",
    "require main"
  ],
  "sideEffects": false,
  "types": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js"
    }
  },
  "dependencies": {
    "default-import": "workspace:^",
    "static-emitter": "workspace:^"
  },
  "devDependencies": {
    "@types/chai": "^4.3.9",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.8.10",
    "@types/sinon": "^10.0.20",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "nx-tsc": "workspace:^",
    "nx-update-ts-references": "workspace:^",
    "nyc": "^15.1.0",
    "sinon": "^17.0.0",
    "typescript": "^5.2.2"
  }
}
