{
  "name": "find-import",
  "version": "1.0.6",
  "description": "Find and load first instance of js/json in parent directories.",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "author": "jacobley",
  "license": "MIT",
  "repository": "jacobley/jacobley",
  "homepage": "https://github.com/JacobLey/jacobley/tree/main/tools/find-import#readme",
  "bugs": {
    "url": "https://github.com/jacobley/jacobley/issues"
  },
  "keywords": [
    "first",
    "parent",
    "load"
  ],
  "sideEffects": false,
  "types": "./dist/find-import.js",
  "exports": {
    ".": {
      "types": "./src/find-import.ts",
      "default": "./dist/find-import.js"
    }
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/find-import.js"
    }
  },
  "dependencies": {
    "parse-cwd": "workspace:^"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.7",
    "chai": "^4.3.8",
    "expect-type": "^0.16.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "typescript": "^5.2.2"
  }
}
