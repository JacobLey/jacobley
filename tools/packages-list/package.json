    "which": "^3.0.1"
  },
  "devDependencies": {
    "@microsoft/rush-lib": "^5.101.1",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.7",
    "@types/sinon": "^10.0.16",
    "@types/which": "^3.0.0",
    "chai": "^4.3.8",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "sinon": "^15.2.0",
    "typescript": "^5.2.2"
  }
