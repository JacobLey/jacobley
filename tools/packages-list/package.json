  },
  "devDependencies": {
    "@microsoft/rush-lib": "^5.110.0",
    "@types/chai": "^4.3.9",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.8.10",
    "@types/sinon": "^10.0.20",
    "@types/which": "^3.0.1",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "nx-tsc": "workspace:^",
    "nx-update-ts-references": "workspace:^",
    "nyc": "^15.1.0",
    "sinon": "^17.0.0",
    "typescript": "^5.2.2"
  }
}
