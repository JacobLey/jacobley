{
  "name": "packages-list",
  "version": "1.1.2",
  "description": "List all packages in a monorepo.",
  "engines": {
    "node": ">=14"
  },
  "type": "module",
  "author": "jacobley",
  "license": "MIT",
  "repository": "jacobley/jacobley",
  "homepage": "https://github.com/JacobLey/jacobley/tree/main/tools/packages-list#readme",
  "bugs": {
    "url": "https://github.com/jacobley/jacobley/issues"
  },
  "keywords": [
    "list",
    "packages",
    "workspace"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "coverage": "rushx instrument && nyc mocha ./nyc/tests/unit",
    "instrument": "nyc instrument --nycrc-path=nyc.instrument.cjs ./dist ./nyc",
    "lint": "rush lint-package",
    "test": "rushx coverage",
    "unit-test": "mocha ./dist/tests/unit"
  },
  "main": "./dist/packages-list.js",
  "exports": {
    ".": {
      "types": [
        "./src/packages-list.ts",
        "./dist/packages-list.d.ts"
      ],
      "node": "./dist/packages-list.js"
    }
  },
  "dependencies": {
    "find-import": "workspace:^",
    "globby": "^13.1.1",
    "named-patch": "workspace:^",
    "parse-cwd": "workspace:^",
    "root-package-json": "workspace:^",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@microsoft/rush-lib": "^5.71.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "@types/sinon": "^10.0.11",
    "@types/which": "^2.0.1",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "typescript": "^4.7.2"
  }
}
