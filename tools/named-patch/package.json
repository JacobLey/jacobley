{
  "name": "named-patch",
  "version": "1.0.8",
  "description": "Higher order function for patching named export functions.",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "author": "jacobley",
  "license": "MIT",
  "repository": "jacobley/jacobley",
  "homepage": "https://github.com/JacobLey/jacobley/tree/main/tools/named-patch#readme",
  "bugs": {
    "url": "https://github.com/jacobley/jacobley/issues"
  },
  "keywords": [
    "higher order function",
    "named export",
    "patch",
    "stub"
  ],
  "sideEffects": false,
  "scripts": {
    "coverage": "rushx instrument && nyc mocha ./nyc/tests/unit",
    "instrument": "nyc instrument --nycrc-path=nyc.instrument.cjs ./dist ./nyc",
    "lint": "rush lint-package",
    "test": "rushx coverage",
    "unit-test": "mocha ./dist/tests/unit"
  },
  "types": "./dist/patch.js",
  "exports": {
    ".": {
      "types": "./src/patch.ts",
      "patchable": "./dist/patch.js",
      "default": "./dist/noop.js"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/patch.js",
        "patchable": "./dist/patch.js",
        "default": "./dist/noop.js"
      }
    }
  },
  "devDependencies": {
    "@types/chai": "^4.3.9",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.8.10",
    "@types/sinon": "^10.0.20",
    "chai": "^4.3.10",
    "expect-type": "^0.17.3",
    "mocha": "^10.2.0",
    "nx-tsc": "workspace:^",
    "nx-update-ts-references": "workspace:^",
    "nyc": "^15.1.0",
    "sinon": "^17.0.0",
    "typescript": "^5.2.2"
  }
}
