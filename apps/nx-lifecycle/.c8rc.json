{
  "reporter": ["html", "text-summary"],
  "exclude": ["coverage/**"],
  "exclude-after-remap": true,
  "extension": [".js", ".cjs", ".mjs", ".ts", ".cts", ".mts", ".tsx", ".jsx"],

  "lines": 100,
  "statements": 100,
  "functions": 100,
  "branches": 100,

  "watermarks": {
    "lines": [100, 95],
    "functions": [100, 95],
    "branches": [100, 95],
    "statements": [100, 95]
  }
}
