{
  "name": "nx-lifecycle",
  "version": "0.0.1",
  "description": "Keep high level nx lifecycle targets in sync",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "author": "jacobley",
  "license": "MIT",
  "repository": "jacobley/jacobley",
  "homepage": "https://github.com/JacobLey/jacobley/tree/main/apps/nx-lifecycle#readme",
  "bugs": {
    "url": "https://github.com/jacobley/jacobley/issues"
  },
  "keywords": [
    "nx",
    "lifecycle",
    "build",
    "test",
    "format",
    "lint"
  ],
