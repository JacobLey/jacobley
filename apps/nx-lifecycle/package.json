    "@nx/devkit": "^16.7.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.7",
    "chai": "^4.3.8",
    "mocha": "^10.2.0",
    "load-populate-files": "workspace:^",
    "nx-populate-files": "workspace:^",
    "nx-tsc": "workspace:^",
    "nyc": "^15.1.0",
    "typescript": "^5.2.2"
