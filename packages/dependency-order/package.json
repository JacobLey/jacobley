{
  "name": "dependency-order",
  "version": "0.0.0",
  "description": "List monorepo packages in dependency order.",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "type": "module",
  "author": "jacobley",
  "license": "MIT",
  "repository": "jacobley/jacobley",
  "homepage": "https://github.com/jacobley/jacobley/packages/dependency-order#readme",
  "bugs": {
    "url": "https://github.com/jacobley/jacobley/issues"
  },
  "keywords": [
    "monorepo",
    "packages",
    "dependency",
    "graph",
    "order"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --build",
    "build:ci": "tsc",
    "coverage": "npm run instrument && nyc mocha ./nyc/tests/unit",
    "instrument": "nyc instrument --nycrc-path=nyc.instrument.cjs ./dist ./nyc",
    "lint": "npm run -w=../.. --include-workspace-root package-lint",
    "lint:fix": "npm run -w=../.. --include-workspace-root package-lint:fix",
    "test": "npm run coverage",
    "unit-test": "mocha ./dist/tests/unit"
  },
  "main": "./dist/dependency-order.js",
  "exports": {
    ".": {
      "types": [
        "./src/dependency-order.ts",
        "./dist/dependency-order.d.ts"
      ],
      "default": "./dist/dependency-order.js"
    }
  },
  "dependencies": {
    "named-patch": "file:../named-patch",
    "packages-list": "file:../packages-list"
  },
  "devDependencies": {
    "chai": "4.3.6",
    "sinon": "14.0.0"
  }
}
