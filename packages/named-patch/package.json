{
  "name": "named-patch",
  "version": "0.0.1",
  "description": "Higher order function for patching named export functions.",
  "engines": {
    "node": ">=14",
    "npm": ">=8"
  },
  "type": "module",
  "author": "jacobley",
  "license": "MIT",
  "repository": "jacobley/jacobley",
  "homepage": "https://github.com/jacobley/jacobley/packages/named-patch#readme",
  "bugs": {
    "url": "https://github.com/jacobley/jacobley/issues"
  },
  "keywords": [
    "higher order function",
    "named export",
    "patch",
    "stub"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --build",
    "build:ci": "tsc",
    "coverage": "npm run instrument && nyc mocha ./nyc/tests/unit",
    "instrument": "nyc instrument --nycrc-path=nyc.instrument.cjs ./dist ./nyc",
    "lint": "npm run -w=../.. --include-workspace-root package-lint",
    "lint:fix": "npm run -w=../.. --include-workspace-root package-lint:fix",
    "test": "npm run coverage",
    "unit-test": "mocha ./dist/tests/unit"
  },
  "main": "./dist/patch.js",
  "exports": {
    ".": {
      "types": [
        "./src/patch.ts",
        "./dist/patch.d.ts"
      ],
      "patchable": "./dist/patch.js",
      "default": "./dist/noop.js"
    }
  },
  "devDependencies": {
    "chai": "4.3.6",
    "expect-type": "0.13.0",
    "sinon": "14.0.0"
  }
}
