{
  "name": "entry-script",
  "version": "0.0.0",
  "description": "Modular control for entry script execution.",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "type": "module",
  "author": "jacobley",
  "license": "MIT",
  "repository": "jacobley/jacobley",
  "homepage": "https://github.com/jacobley/jacobley/packages/entry-script#readme",
  "bugs": {
    "url": "https://github.com/jacobley/jacobley/issues"
  },
  "keywords": [
    "executable",
    "entry point",
    "require main"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --build",
    "build:ci": "tsc",
    "coverage": "npm run instrument && nyc mocha ./nyc/tests/unit",
    "instrument": "nyc instrument --nycrc-path=nyc.instrument.cjs ./dist ./nyc",
    "lint": "npm run -w=../.. --include-workspace-root package-lint",
    "lint:fix": "npm run -w=../.. --include-workspace-root package-lint:fix",
    "test": "npm run coverage",
    "unit-test": "mocha ./dist/tests/unit"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "default-import": "file:../default-import",
    "static-emitter": "file:../static-emitter"
  },
  "devDependencies": {
    "chai": "4.3.6",
    "sinon": "14.0.0"
  }
}
