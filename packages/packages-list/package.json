{
  "name": "packages-list",
  "version": "0.0.0",
  "description": "List all packages in a monorepo.",
  "engines": {
    "node": ">=14",
    "npm": ">=8"
  },
  "type": "module",
  "author": "jacobley",
  "license": "MIT",
  "repository": "jacobley/jacobley",
  "homepage": "https://github.com/jacobley/jacobley/packages/packages-list#readme",
  "bugs": {
    "url": "https://github.com/jacobley/jacobley/issues"
  },
  "keywords": [
    "list",
    "packages",
    "workspace"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --build",
    "build:ci": "tsc",
    "coverage": "npm run instrument && nyc mocha ./nyc/tests/unit",
    "instrument": "nyc instrument --nycrc-path=nyc.instrument.cjs ./dist ./nyc",
    "lint": "npm run -w=../.. --include-workspace-root package-lint",
    "lint:fix": "npm run -w=../.. --include-workspace-root package-lint:fix",
    "test": "npm run coverage",
    "unit-test": "mocha ./dist/tests/unit"
  },
  "main": "./dist/packages-list.js",
  "exports": {
    ".": {
      "types": [
        "./src/packages-list.ts",
        "./dist/packages-list.d.ts"
      ],
      "node": "./dist/packages-list.js"
    }
  },
  "dependencies": {
    "globby": "^13.1.1",
    "named-patch": "file:../named-patch",
    "root-package-json": "file:../root-package-json"
  },
  "devDependencies": {
    "chai": "4.3.6",
    "sinon": "14.0.0"
  }
}
