{
  "name": "parse-cwd",
  "version": "0.0.0",
  "description": "Convert relative path or URL to full path",
  "engines": {
    "node": ">=14",
    "npm": ">=8"
  },
  "type": "module",
  "author": "jacobley",
  "license": "MIT",
  "repository": "jacobley/jacobley",
  "homepage": "https://github.com/jacobley/jacobley/packages/parse-cwd#readme",
  "bugs": {
    "url": "https://github.com/jacobley/jacobley/issues"
  },
  "keywords": [
    "cwd",
    "resolve",
    "file path"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --build",
    "build:ci": "tsc",
    "coverage": "npm run instrument && nyc mocha ./nyc/tests/unit",
    "instrument": "nyc instrument --nycrc-path=nyc.instrument.cjs ./dist ./nyc",
    "lint": "npm run -w=../.. --include-workspace-root package-lint",
    "lint:fix": "npm run -w=../.. --include-workspace-root package-lint:fix",
    "test": "npm run coverage",
    "unit-test": "mocha ./dist/tests/unit"
  },
  "main": "./dist/parse-cwd.js",
  "exports": {
    ".": {
      "types": [
        "./src/parse-cwd.ts",
        "./dist/parse-cwd.d.ts"
      ],
      "node": "./dist/parse-cwd.js"
    }
  },
  "devDependencies": {
    "chai": "4.3.6"
  }
}
