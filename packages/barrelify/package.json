{
  "name": "barrelify",
  "version": "0.0.1",
  "description": "Auto-generate TS barrel files.",
  "engines": {
    "node": ">=14",
    "npm": ">=8"
  },
  "type": "module",
  "author": "jacobley",
  "license": "MIT",
  "repository": "jacobley/jacobley",
  "homepage": "https://github.com/jacobley/jacobley/packages/barrelify#readme",
  "bugs": {
    "url": "https://github.com/jacobley/jacobley/issues"
  },
  "keywords": [
    "barrel",
    "index"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --build",
    "build:ci": "tsc",
    "coverage": "npm run instrument && nyc mocha ./nyc/tests/unit",
    "instrument": "nyc instrument --nycrc-path=nyc.instrument.cjs ./dist ./nyc",
    "lint": "npm run -w=../.. --include-workspace-root package-lint",
    "lint:fix": "npm run -w=../.. --include-workspace-root package-lint:fix",
    "test": "npm run coverage",
    "unit-test": "mocha ./dist/tests/unit"
  },
  "main": "./dist/barrelify.js",
  "exports": {
    ".": {
      "types": [
        "./src/barrelify.ts",
        "./dist/barrelify.d.ts"
      ],
      "default": "./dist/barrelify.js"
    }
  },
  "bin": {
    "barrel": "./cli.js",
    "barrelify": "./cli.js"
  },
  "dependencies": {
    "default-import": "file:../default-import",
    "entry-script": "file:../entry-script",
    "globby": "^13.1.1",
    "named-patch": "file:../named-patch",
    "parse-cwd": "file:../parse-cwd",
    "read-pkg-up": "^9.1.0",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "chai": "4.3.6",
    "dedent": "^0.7.0",
    "expect-type": "0.13.0",
    "sinon": "14.0.0"
  }
}
