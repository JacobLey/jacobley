    "rivendell": "rivendell-dist",
    "test": "echo NO TESTS YET",
    "unit-test": "mocha ./dist/tests/unit"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.1.3",
    "@types/node": "17.0.35",
    "@types/yargs": "17.0.10",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "default-import": "workspace:^",
    "dependency-order": "workspace:^",
    "entry-script": "workspace:^",
    "eslint": "8.16.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-sonarjs": "0.13.0",
    "eslint-plugin-unicorn": "42.0.0",
    "named-patch": "workspace:^",
    "packages-list": "workspace:^",
    "rivendell": "workspace:^",
    "rivendell-dist": "npm:rivendell@0.3.0",
    "typescript": "4.7.1-rc",
    "yargs": "^17.4.1"
  },
  "overrides": {
    "typescript": "4.7.1-rc"
  }
