{
  "name": "jacobley",
  "version": "0.0.0",
  "description": "Monorepo root.",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "private": true,
  "type": "module",
  "author": "jacobley",
  "license": "MIT",
  "repository": "jacobley/jacobley",
  "homepage": "https://github.com/jacobley/jacobley#readme",
  "bugs": {
    "url": "https://github.com/jacobley/jacobley/issues"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "coverage": "rushx instrument && nyc mocha ./nyc/tests/unit",
